    "typescript": "^4.4.4"
  },
  "scripts": {
    "accounts": "npx hardhat accounts",
    "compile": "npx hardhat compile",
    "clean": "npx hardhat clean",
    "node": "npx hardhat node",
    "help": "npx hardhat help",
    "test": "cross-env REPORT_GAS=true npx hardhat test",
    "coverage": "npx hardhat coverage",
    "deploy": "npx hardhat run scripts/deploy.ts",
    "deploy_ts": "cross-env TS_NODE_FILES=true npx ts-node scripts/deploy.ts",
    "lint": "npx eslint **/*.{js,ts}",
    "lintFix": "npx eslint **/*.{js,ts} --fix",
    "pretty": "npx prettier **/*.{json,sol,md} --check",
    "prettyFix": "npx prettier **/*.{json,sol,md} --write",
    "solhint": "npx solhint contracts/**/*.sol",
    "solhintFix": "npx solhint contracts/**/*.sol --fix"
  }
