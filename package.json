    "@types/mocha": "^9.0.0",
    "@types/node": "^16.11.7",
    "@typescript-eslint/eslint-plugin": "^5.3.1",
    "@typescript-eslint/parser": "5.0.0",
    "chai": "^4.3.4",
    "cross-env": "^7.0.3",
    "eslint": "^8.2.0",
    "ethereum-waffle": "3.2.0",
    "ethers": "5.1.3",
    "hardhat": "^2.6.8",
    "hardhat-gas-reporter": "^1.0.4",
    "lodash": "4.17.21",
    "mocha": "^9.1.3",
    "solhint": "^3.3.6",
